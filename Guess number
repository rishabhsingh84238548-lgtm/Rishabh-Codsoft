import java.util.Random;
import java.util.*;
class GuessNumber {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        Random r=new Random();
        int score=0;
        while(true){
        int userguessnum;
        int num=r.nextInt(101);
        int attempt=0;
        boolean guesscorrect=false;
        System.out.println("numbers between 1 to 100:");
while(attempt<6){
            System.out.println("enter guessed number:");
            userguessnum=sc.nextInt();
            attempt++;
            if(userguessnum>num)
            {
                System.out.println(" too high number is guessed");

            }
           else if(userguessnum<num){
                System.out.println("low number!");
            } else {
                System.out.println("correct number is guessed");
                guesscorrect=true;
                break;
            }
        }
            System.out.println("want to play again");
char playagain=sc.next().charAt(0);
if(playagain=='x'||playagain=='N'){
    break;
}
    }}}
